{
	"compilerOptions": {
		"baseUrl": ".",
		"strict": true,
		"target": "ES6",
		"module": "ES6",
		"moduleResolution": "Node",
		"sourceMap": true,
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
		"paths": {
			"@core/*": ["core/*"],
			"@config/*": ["config/*"],
			"@constants/*": ["constants/*"],
			"@data/*": ["data/*"],
			"@interfaces/*": ["interfaces/*"],
			"@pages/*": ["pages/*"],
			"@fixtures/*": ["fixtures/*"]
		}
	}
}
